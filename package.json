{
  "devDependencies": {
    "@testing-library/cypress": "^10.0.2",
    "cypress": "^13.17.0",
    "cypress-multi-reporters": "^1.6.4",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "opn-cli": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "e2e:chrome": "cypress run --browser chrome",
